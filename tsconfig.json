{
    "version": "1.4.1",
    "compilerOptions": {
        "target": "es5",
        "module": "commonjs",
        "declaration": false,
        "noImplicitAny": false,
        "removeComments": true,
        "noLib": false,
        "preserveConstEnums": true,
        "suppressImplicitAnyIndexErrors": true,
        "noEmitOnError": true
    },
    "filesGlob": [
        "./**/*.ts",
        "!./node_modules/**/*.ts"
    ],
    "files": [
        "./src/browser/index.ts",
        "./src/command.ts",
        "./src/environment.ts",
        "./src/index.ts",
        "./src/shellModel.ts",
        "./typings/backbone/backbone.d.ts",
        "./typings/github-electron/github-electron.d.ts",
        "./typings/jquery/jquery.d.ts",
        "./typings/node/node.d.ts",
        "./typings/requirejs/require.d.ts",
        "./typings/tsd.d.ts",
        "./typings/underscore/underscore.d.ts"
    ]
}
