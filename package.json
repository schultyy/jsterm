{
  "name": "jsterm",
  "productName": "Jsterm",
  "version": "0.0.0",
  "description": "JS Terminal Emulator",
  "license": "MIT",
  "main": "index.js",
  "repository": "schultyy/jsterm",
  "author": {
    "name": "Jan Schulte",
    "email": "hello@unexpected-co.de",
    "url": "github.com/schultyy/jsterm"
  },
  "engines": {
    "node": ">=0.10.0"
  },
  "scripts": {
    "prestart": "tsc src/**/*.ts --target es6",
    "start": "electron src/",
